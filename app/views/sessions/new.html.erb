<div class = "log">
<h1>Finance Admin Login</h1>
<p id="alert"><%= alert %></p>
<%= form_tag sessions_path do |form| %>
  <div class="field">
    <%= label_tag :email %>
    <%= text_field_tag :email,'', :required => 'true', :placeholder => "username"  %>
  </div>
  <div class="field">
    <%= label_tag :password %>
    <%= password_field_tag :password,'', :required => 'true', :placeholder => "password" %>
  </div>
  <div class="actions">
    <%= submit_tag "Login" %>
  </div>
<% end %>
</div>
